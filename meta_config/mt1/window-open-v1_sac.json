{
    "env_name" : "mt10",
    "env":{
        "reward_scale":1,
        "obs_norm":false
    },
    "meta_env":{
        "obs_type": "with_goal",
        "random_init": true
    },
    "replay_buffer":{
        "size": 1e6
    },
    "net":{ 
        "hidden_shapes": [400, 400],
        "q_hidden_shapes":[400, 400],
        "append_hidden_shapes":[]
    },
    "general_setting": {
        "discount" : 0.99,
        "pretrain_epochs" : 20,
        "num_epochs" : 5000,
        "epoch_frames" : 200,
        "max_episode_frames" : 200,

        "batch_size" : 128,
        "min_pool" : 10000,

        "target_hard_update_period" : 1000,
        "use_soft_update" : true,
        "tau" : 0.002,
        "opt_times" : 200,

        "eval_episodes" : 10
    },
    "sac":{
        
        "plr" : 2.5e-4,
        "qlr" : 2.5e-4,
        
        "reparameterization": true,
        "automatic_entropy_tuning": true,
        "policy_std_reg_weight": 0,
        "policy_mean_reg_weight": 0
    }
}
